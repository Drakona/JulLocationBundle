<script type="text/javascript" src="{{ asset('bundles/jullocation/js/placesAutocomplete.js') }}"></script>
<script type="text/javascript">

/**
 * Call places autocomplete
 */
function JulAutoComplete()
{
	return GmapInit( '{{ mapDiv }}', {{ mapOptions|raw }}, {{ acFields|raw }}, '{{ topLevel }}', {{ zoomResolved|default(0) }}, {{ latitude|default(0) }}, {{ longitude|default(0) }}, '{% trans %}photo.selector{% endtrans %}', '{% trans %}photo.selected{% endtrans %}', {{ maxImageWidth|default(200) }}, {{ maxImageHeight|default(200) }}, {{ jsFieldIds|raw }} );
}
</script>