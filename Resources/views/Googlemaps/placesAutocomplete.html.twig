<script type="text/javascript" src="{{ asset('bundles/jullocation/js/placesAutocomplete.js') }}"></script>
<script type="text/javascript">

/**
 * Call places autocomplete
 */
function JulAutoComplete()
{
	GmapInit( '{{ mapDiv }}', {{ mapOptions|raw }}, {{ acFields|raw }}, '{{ topLevel }}', {{ zoomResolved }}, {{ latitude }}, {{ longitude }}, '{% trans %}photo.selector{% endtrans %}', '{% trans %}photo.selected{% endtrans %}', {{ maxImageWidth }}, {{ maxImageHeight }}, {{ jsFieldIds|raw }} );
}
</script>